{
  "name": "@data-fair/processings",
  "version": "1.23.5",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build-types": "df-build-types api && df-build-types worker",
    "build-types-api": "df-build-types api",
    "build-types-worker": "df-build-types worker",
    "dev-api": "npm -w api run dev",
    "dev-deps": "docker compose --profile dev up -d --wait",
    "dev-ui": "npm -w ui run dev",
    "dev-worker": "npm -w worker run dev",
    "dev-zellij": "export DEV_SHELL=$(basename \"$SHELL\") && zellij --layout .zellij.kdl",
    "i": "rimraf node_modules ui/node_modules api/node_modules worker/node_modules && npm i --force && cd api && npm i --force && cd ../ui && npm i --force && cd ../worker && npm i --force",
    "lint": "eslint --ext js,vue --ignore-path .gitignore .",
    "lint-fix": "eslint --ext js,vue --ignore-path .gitignore --fix .",
    "test-outdated": "NODE_ENV=test mocha --exit --bail --timeout 10000 test/",
    "test": "node test-it/index.js",
    "test-deps": "docker compose --profile test up -d --wait",
    "stop-dev-deps": "docker compose --profile dev stop"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/data-fair/processings.git"
  },
  "author": "",
  "license": "AGPL-3.0-only",
  "workspaces": [
    "ui",
    "api",
    "worker"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "plugins": [
      "no-only-tests"
    ],
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "standard",
      "plugin:vue/recommended"
    ],
    "rules": {
      "vue/no-v-html": "off",
      "vue/multi-word-component-names": "off",
      "node/no-deprecated-api": "off",
      "vue/no-mutating-props": "off",
      "vue/require-prop-types": "off",
      "vue/no-useless-template-attributes": "off"
    }
  },
  "bugs": {
    "url": "https://github.com/data-fair/processings/issues"
  },
  "homepage": "https://github.com/data-fair/processings#readme",
  "devDependencies": {
    "@bcherny/json-schema-ref-parser": "^10.0.5-fork",
    "babel-eslint": "^10.0.2",
    "clean-modules": "^3.0.4",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-typescript": "^3.0.0",
    "fast-json-stringify": "^5.12.0",
    "json-schema-to-typescript": "^11.0.5",
    "nock": "^13.0.6",
    "nodemon": "^1.19.3",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "@data-fair/lib": "0.26.0",
    "agentkeepalive": "4",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "ajv-i18n": "^4.2.0",
    "axios": "^1.6.0",
    "config": "^3.3.6",
    "crypto-random-string": "^3.3.0",
    "debug": "^4.3.1",
    "fs-extra": "^8.1.0",
    "gtfs-realtime-bindings": "0.0.5",
    "mongodb": "6",
    "nanoid": "^5.0.6",
    "pump": "^3.0.0",
    "resolve-path": "^1.4.0",
    "semver": "^7.3.4",
    "tmp-promise": "^2.1.1"
  }
}
