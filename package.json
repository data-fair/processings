{
  "name": "@data-fair/processings",
  "version": "1.23.5",
  "description": "",
  "main": "index.js",
  "scripts": {
    "analyze": "nuxt build --analyze",
    "build": "nuxt build",
    "dev-api": "npm -w api run dev",
    "dev-deps": "docker compose --profile dev up -d --wait",
    "dev-ui": "npm -w ui run dev",
    "i": "rimraf node_modules api/node_modules ui/node_modules && npm i && cd api && npm i && cd ../ui && npm i",
    "lint": "eslint --ext js,vue --ignore-path .gitignore .",
    "lint-fix": "eslint --ext js,vue --ignore-path .gitignore --fix .",
    "test": "NODE_ENV=test mocha --exit --bail --timeout 10000 test/",
    "test-deps": "docker compose --profile test up -d --wait",
    "stop-dev-deps": "docker compose --profile dev stop"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/data-fair/processings.git"
  },
  "author": "",
  "license": "AGPL-3.0-only",
  "workspaces": [
    "ui",
    "api"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "plugins": [
      "no-only-tests"
    ],
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "standard",
      "plugin:vue/recommended"
    ],
    "rules": {
      "vue/no-v-html": "off",
      "vue/multi-word-component-names": "off",
      "node/no-deprecated-api": "off",
      "vue/no-mutating-props": "off",
      "vue/require-prop-types": "off",
      "vue/no-useless-template-attributes": "off"
    }
  },
  "bugs": {
    "url": "https://github.com/data-fair/processings/issues"
  },
  "homepage": "https://github.com/data-fair/processings#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "clean-modules": "^2.0.4",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "mocha": "^8.2.1",
    "nock": "^13.0.6",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "@data-fair/sd-express": "1.4.3",
    "axios": "^1.6.0",
    "child-process-promise": "^2.2.1",
    "config": "^3.3.6",
    "crypto-random-string": "^3.3.0",
    "debug": "^4.3.1",
    "event-to-promise": "^0.8.0",
    "fs-extra": "^8.1.0",
    "gtfs-realtime-bindings": "0.0.5",
    "pump": "^3.0.0",
    "resolve-path": "^1.4.0",
    "semver": "^7.3.4",
    "tmp-promise": "^2.1.1",
    "tree-kill": "^1.2.2",
    "ws": "^8.9.0"
  }
}
