{
  "$id": "https://github.com/data-fair/processings/api/config",
  "x-exports": [
    "types",
    "validate"
  ],
  "x-ajv": {
    "coerceTypes": "array"
  },
  "type": "object",
  "title": "Api config",
  "additionalProperties": false,
  "required": [
    "dataDir",
    "defaultTimeZone",
    "directoryUrl",
    "mongoUrl",
    "origin",
    "port",
    "observer"
  ],
  "properties": {
    "dataDir": {
      "type": "string"
    },
    "tmpDir": {
      "type": "string"
    },
    "defaultLimits": {
      "type": "object",
      "properties": {
        "processingsSeconds": {
          "type": "number"
        }
      }
    },
    "defaultTimeZone": {
      "type": "string"
    },
    "directoryUrl": {
      "type": "string",
      "pattern": "^https?://"
    },
    "mongoUrl": {
      "type": "string"
    },
    "origin": {
      "type": "string",
      "pattern": "^https?://"
    },
    "port": {
      "type": "number"
    },
    "privateDirectoryUrl": {
      "type": "string"
    },
    "observer": {
      "type": "object",
      "properties": {
        "active": {
          "type": "boolean"
        },
        "port": {
          "type": "number"
        }
      }
    },
    "util": {},
    "get": {},
    "has": {}
  }
}